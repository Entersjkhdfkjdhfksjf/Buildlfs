# Pkgfile for network-manager-applet

name=network-manager-applet
version=1.32.0
release=1
source=(https://download.gnome.org/sources/$name/1.32/$name-$version.tar.xz)
build_opt="-Dappindicator=no -Dselinux=false"

post_build() {
	rm $PKG/usr/share/glib-2.0/schemas/org.gnome.nm-applet.gschema.xml
}
